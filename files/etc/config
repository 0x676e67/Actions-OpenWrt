config led 'led_wan'
	option name 'WAN'
	option sysfs 'green:wan'
	option trigger 'netdev'
	option mode 'link tx rx'
	option default '0'
	option dev 'eth1'

config led 'led_lan'
	option name 'LAN'
	option sysfs 'green:lan'
	option trigger 'netdev'
	option mode 'link tx rx'
	option default '0'
	option dev 'eth0'